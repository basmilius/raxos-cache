{
    "name": "raxos/cache",
    "description": "Cache clients and utilities.",
    "license": "mit",
    "minimum-stability": "dev",
    "prefer-stable" : true,
    "type": "library",
    "authors": [
        {
            "name": "Bas Milius",
            "email": "bas@mili.us"
        }
    ],
    "autoload": {
        "psr-4": {
            "Raxos\\Cache\\": "src/Raxos/Cache"
        }
    },
    "config": {
        "platform": {
            "php": "8.3"
        }
    },
    "repositories": [
        {
            "type": "path",
            "url": "../*",
            "symlink": true
        }
    ],
    "require": {
        "ext-redis": "*",
        "jetbrains/phpstorm-attributes": "^1.1",
        "raxos/foundation": "*"
    }
}
